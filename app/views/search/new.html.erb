<div class="container">
  <div class="Park_Search">
    <h1>List of Parks Nearby</h1>
    <% @parks.each do |park| %>
      <%= link_to park_activities_path(park.id), :action => 'go'  do %>
        <h3><%= park.name %></h3>
        <p><%= park.address %></p>
      <% end %>
    <% end %>
  </div>

  <div class="Activity Search">
    <h1>List of Activities</h1>
    <% @activities.each do |activity| %>
    <%= link_to park_activities_path(activity.id), :action => 'go'  do %>
        <h3><%= activity.name %></h3>
      <% end %>
    <% end %>
  </div>
</div>
