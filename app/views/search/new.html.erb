<div class="panel-group">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" href="#collapse1">Search by Park</a>
      </h4>
    </div>
    <div id="collapse1" class="panel-collapse collapse">
      <div class="panel-body" id="parks_near_by"></div>
    </div>
  </div>
</div>

<div class="panel-group">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" href="#collapse2">Search by Activities</a>
      </h4>
    </div>
    <div id="collapse2" class="panel-collapse collapse">
      <div class="panel-body">
        <div class="Activity Search">
          <% @activities.each do |activity| %>
            <%= link_to park_list_path(activity.id), :action => 'go'  do %>
              <h3><%= activity.name %></h3>
            <% end %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>





